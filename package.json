{
  "name": "hvweb",
  "version": "1.0.0",
  "description": "A generated Bluemix application",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^6.9.0"
  },
  "scripts": {
    "build-css": "node-sass src/app/assets/scss -o src/app/assets/css --include-path src/app/assets/libs",
    "watch-css": "npm run build-css && node-sass src/app/assets/scss -o src/app/assets/css  --include-path src/app/assets/libs --watch --recursive",
    "start-js": "PORT=4000 react-scripts start",
    "start-dev": "./node_modules/.bin/npm-run-all -p watch-css start-js",
    "start": "./node_modules/.bin/npm-run-all start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "appmetrics-dash": "^3.4.0",
    "axios": "^0.16.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "bluemix-appid": "^1.1.2",
    "bluemix-autoscaling-agent": "^1.0.12",
    "bluemix-objectstorage": "*",
    "body-parser": "^1.18.2",
    "bulma": "^0.3.2",
    "cfenv": "^1.0.3",
    "cloudant": "^1.7.1",
    "css-loader": "^0.26.2",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "foundation-sites": "^6.4.3",
    "gulp": "^3.9.1",
    "gulp-sass": "^3.1.0",
    "gulp-shell": "^0.6.5",
    "lodash": "^4.17.4",
    "npm-run-all": "^4.1.2",
    "passport": "^0.3.2",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.9",
    "weather-icons": "^1.3.2",
    "webpack": "^2.7.0",
    "webpack-stream": "^3.2.0"
  },
  "devDependencies": {
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "react-scripts": "^1.0.17",
    "webpack-dev-server": "^2.9.7"
  }
}
